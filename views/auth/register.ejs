<h1>Register</h1>
<div>
    <form action="/user/register" method="POST" enctype="multipart/form-data">
        <div>
            <label for=""></label>
            <input type="text" name="userName" placeholder="User Name" class="form-control">
        </div>
        <div>
            <label for=""></label>
            <input type="password" name="password" placeholder="Password" class="form-control">
        </div>
        <div>
            <label for=""></label>
            <input type="text" name="firstName" placeholder="First Name" class="form-control">
        </div>
        <div>
            <label for=""></label>
            <input type="text" name="lastName" placeholder="Last Name" class="form-control">
        </div>
        <div>
            <label for=""></label>
            <input type="date" name="birthday" placeholder="Date Of Birth" class="form-control">
        </div>
        <div>
            <label for=""></label>
            <input type="text" name="phoneNo" placeholder="Phone" class="form-control">
        </div>
        <div>
            <label for=""></label>
            <input type="email" name="email" placeholder="Email" class="form-control">
        </div>
        <div>
            <label for=""></label>
            <input type="text" name="address" placeholder="Address" class="form-control">
        </div>
        <div>
            <label for=""></label>
            <input type="file" name="profilePicture" placeholder="Profile Picture" class="form-control">
        </div>


        <button type="submit">Register</button>
    </form>
</div>